<%-include('../layouts/admin/styles')-%>

   <%-include('../admin/header')-%>
      <div class="conatiner-fluid content-inner mt-n5 py-0">
         <div>
            <div class="row">
               <div class="col-xl-12 col-lg-12 mx-auto">
                  <div class="card">
                     <div class="card-header d-flex justify-content-between">
                        <div class="header-title">
                           <h4 class="card-title">New Coupon Information</h4>
                        </div>
                     </div>
                     <div class="card-body">
                        <div class="new-user-info">
                           <form method="post">
                           <div class="row">
                              <div class="form-group col-md-3">
                                 <label class="form-label" for="pcode">Type:</label>
                                 <input required name="pcode" type="text" class="form-control" id="pcode"
                                    placeholder="Coupon Code">
                              </div>
                              <div class="form-group col-md-3">
                               
                                 <label class="form-label" for="pname">Calculation:</label>
                                 <input required name="pname" type="number" class="form-control" id="pname"
                                    placeholder="Coupon Calculation">
                              </div>
                              <div class="form-group col-md-3">
                                 <label class="form-label" for="type">Calculation type:</label>
                                 <input required name="type" type="text" class="form-control" id="calc_type"
                                    placeholder="calculaton type">
                              </div>
                              <div class="form-group col-md-3">
                                 <label class="form-label" for="quantity">Quantity:</label>
                                 <input required name="quantity" type="number" class="form-control" id="quantity"
                                    placeholder="Quantity">
                              </div>
                              
                              <div class="form-group col-md-4">
                                 <label class="form-label" for="cname">Coupon Description:</label>
                                 <input required name="cname" type="text" class="form-control" id="categorySelect"
                                    placeholder="Coupon Description">
                          
                              </input>
                              </div>
                             
            
                             
                             
                             
                              <div class="form-group col-md-4">
                                 <label class="form-label" for="price">Price:</label>
                                 <input required name="price" type="text" class="form-control" id="price"
                                    placeholder="Price">
                              </div>
                              <div class="form-group col-md-4 ">
                                 <hr>
                                 <button type="submit" class="btn btn-primary">Add New Coupon</button>
                              </div>
                           </div>
                           <% if(typeof message !=='undefined' ){ %>
                              <h5 class="text-center text-success">
                                 <%= message %>
                              </h5>
                              <% } %>
                                 </form>
                        </div>
                     </div>
                  </div>
               </div>
